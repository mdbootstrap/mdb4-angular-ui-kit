{
  "name": "angular-bootstrap-md-lib",
  "version": "7.0.0",
  "description": "Bootstrap Material Design for Angular",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve mdb-angular",
    "build": "ng build mdb-angular",
    "test": "ng test mdb-angular",
    "lint": "ng lint mdb-angular",
    "e2e": "ng e2e mdb-angular",
    "build:lib": "gulp --gulpfile gulpfile.lib.js",
    "pack": "cp -r src/angular-bootstrap-md/scss src/angular-bootstrap-md/font/ src/angular-bootstrap-md/img/ dist && cp README.md ./dist && cd dist && npm pack && cd .. && npm run copy",
    "copy": "cp dist/angular-bootstrap-md-7.* ./",
    "compile:lib": "npm run build:lib && npm run pack",
    "version": "sync-json -v --property version --source package.json src/package.json",
    "remove-demo-unix": "rm -rf projects && rm angular.json && mv angular-clean.json angular.json",
    "remove-demo-windows": "rmdir /s /q projects && del angular.json && ren angular-clean.json angular.json",
    "schematics:build": "tsc -p projects/schematics/tsconfig.json",
    "schematics:copy": "npm run schematics:build && webpack",
    "compile": "npm run compile:lib && npm run schematics:copy"
  },
  "dependencies": {
    "@agm/core": "1.0.0-beta.5",
    "@angular-devkit/build-optimizer": "^0.10.5",
