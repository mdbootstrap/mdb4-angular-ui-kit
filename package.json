{
  "name": "angular-bootstrap-md-lib",
  "version": "6.2.5",
  "description": "Bootstrap Material Design for Angular",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "gulp --gulpfile gulpfile.lib.js",
    "pack": "cp -r src/angular-bootstrap-md/scss src/angular-bootstrap-md/font/ src/angular-bootstrap-md/img/ dist && cd dist && npm pack && cd ..",
    "copy": "cp dist/angular-bootstrap-md-6.* ./ && cp README.md ./dist",
    "compile": "npm run build:lib && npm run pack && npm run copy",
    "version": "sync-json -v --property version --source package.json src/package.json",
    "remove-demo-unix": "rm -rf projects && rm angular.json && mv angular-clean.json angular.json",
    "remove-demo-windows": "rmdir /s /q projects && del angular.json && ren angular-clean.json angular.json"
  },
  "dependencies": {
    "@angular-devkit/build-optimizer": "^0.6.8",
    "@angular/animations": "^6.0.0-rc.1",
    "@angular/common": "^6.0.0-rc.1",
    "@angular/compiler": "^6.0.0-rc.1",
    "@angular/core": "^6.0.0-rc.1",
    "@angular/forms": "^6.0.0-rc.1",
    "@angular/platform-server": "^6.0.0-rc.1",
    "@angular/http": "^6.0.0-rc.1",
    "@angular/platform-browser": "^6.0.0-rc.1",
    "@angular/platform-browser-dynamic": "^6.0.0-rc.1",
    "@angular/router": "^6.0.0-rc.1",
    "angular-bootstrap-md": "file:angular-bootstrap-md-6.2.5.tgz",
    "@agm/core": "1.0.0-beta.5",
    "chart.js": "^2.7.2",
    "core-js": "^2.4.1",
    "font-awesome": "4.7.x",
    "fs-extra": "^6.0.1",
    "rxjs": "^6.0.0-rc.1",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.5.0",
    "@angular-devkit/core": "^0.5.7",
    "@angular/cli": "^6.0.0-rc.1",
    "@angular/compiler-cli": "^6.0.0-rc.1",
    "@angular/language-service": "^6.0.0-rc.1",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.0.1",
    "del": "^3.0.0",
