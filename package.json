    "@angular/router": "^7.0.3",
    "@types/chart.js": "^2.7.40",
    "angular-bootstrap-md": "file:angular-bootstrap-md-7.0.0.tgz",
    "chart.js": "^2.7.2",
    "core-js": "^2.4.1",
    "font-awesome": "4.7.x",
    "fs-extra": "^7.0.1",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.0.0-rc.1",
    "tsickle": "^0.33.1",
    "webpack-cli": "^3.1.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.5",
